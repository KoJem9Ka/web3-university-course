  "devDependencies": {
    "@maticnetwork/maticjs": "^3.6.7",
    "@openzeppelin/contracts": "^5.0.0",
    "@truffle/hdwallet-provider": "^2.1.15",
    "dotenv": "^16.3.1",
    "gh-pages": "^6.0.0",
    "truffle-plugin-verify": "^0.6.6"
